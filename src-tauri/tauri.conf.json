  "productName": "COS72-tauri",
  "version": "0.13.0",
  "identifier": "com.COS72-tauri.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
